<!-- This is the base xml for all physics simulations. Set global configs here. -->
<mujoco model="base">
  <compiler angle="radian" meshdir="meshes/" inertiagrouprange="0 0" autolimits="true"/>
  <option impratio="20" cone="elliptic" density="1.2" viscosity="0.00002" solver="CG" integrator="implicitfast"/>
  <size nconmax="5000" njmax="5000"/>
  <extension>
    <plugin plugin="mujoco.elasticity.membrane"/>
    <plugin plugin="mujoco.sdf.sqsocket">
      <instance name="sqsocket" />
    </plugin>
  </extension>

  <asset>
  </asset>

  <visual>
    <map znear="0.001" />
  </visual>

  <actuator>
  </actuator>

  <worldbody>
  </worldbody>

  <equality>
  </equality>
</mujoco>