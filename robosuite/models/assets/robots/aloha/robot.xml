              <geom quat="0 1 0 0" class="collision" mesh="vx300s_5_lower_forearm"/>
              <body name="left_wrist_link" pos="0.1 0 0">
                <inertial pos="0.046743 -7.6652e-06 0.010565" quat="-0.00100191 0.544586 0.0026583 0.8387"
