<mujoco model="ur5_rg2">
  <asset>
    <mesh name="hand" file="meshes/onrobot_gripper_rg2/hand.stl"/>
    <mesh name="finger" file="meshes/onrobot_gripper_rg2/finger.stl"/>
  </asset>

  <actuator>
    <position ctrllimited="true" ctrlrange="0 1.18" joint="rg2_finger_joint1" kp="1000" name="gripper_l_finger_joint" forcelimited="true" forcerange="-20 20"/>
    <position ctrllimited="true" ctrlrange="0 1.18" joint="rg2_finger_joint2" kp="1000" name="gripper_r_finger_joint" forcelimited="true" forcerange="-20 20"/>
  </actuator>

  <worldbody>
    <body name="gripper_base" pos="0 0 0">

    <site name="ft_frame" pos="0 0 0" size="0.01 0.01 0.01" rgba="1 0 0 1" type="sphere" group="1" />

      <body name="eef" pos="0 0 0.09465">

        <!-- This site was added for visualization. -->
        <site name="grip_site" pos="0 0 0" size="0.01 0.01 0.01" rgba="1 0 0 0.5" type="sphere" group="1"/>
        <!-- This site was added for visualization. -->
        <site name="grip_site_cylinder" pos="0 0 0.1399" size="0.005 10" rgba="0 1 0 0.3" type="cylinder" group="1"/>

        <geom name="rg2_hand_collision" pos="0 0.0823 0" quat="0.5 0.5 0.5 0.5" type="mesh" mesh="hand" group="0"/>

        <body name="rg2_leftfinger" pos="1.57951e-10 0.1873 0.017" quat="0.5 0.5 0.5 0.5">
          <joint name="rg2_finger_joint1" pos="0 0 0" axis="0 0 1" limited="true" range="0 1.18"/>
          <geom type="mesh" mesh="finger"/>
        </body>

        <body name="rg2_rightfinger" pos="2.18977e-10 0.1873 -0.017" quat="0.5 0.5 0.5 0.5">
          <joint name="rg2_finger_joint2" pos="0 0 0" axis="0 0 -1" limited="true" range="0 1.18"/>
          <geom quat="0 1 0 0" type="mesh" mesh="finger"/>
        </body>

      </body>
    </body>
  </worldbody>
</mujoco>
