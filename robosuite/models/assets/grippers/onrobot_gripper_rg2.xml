<mujoco model="ur5_rg2">
  <compiler angle="radian" autolimits="true"/>
  <asset>
    <mesh name="hand" file="meshes/onrobot_gripper_rg2/hand.stl"/>
    <mesh name="hand_vis" file="meshes/onrobot_gripper_rg2/hand_vis.stl"/>
    <mesh name="finger" file="meshes/onrobot_gripper_rg2/finger.stl"/>
    <mesh name="finger_vis" file="meshes/onrobot_gripper_rg2/finger_vis.stl"/>
  </asset>

  <default>
    <default class="visual">
      <geom group="1" type="mesh" conaffinity="0" contype="0" density="0" rgba="1 1 1 1"/>
      <site group="1"/>
    </default>

    <default class="collision">
      <geom group="0" type="mesh"/>
      <site group="0"/>
    </default>

    <default class="finger_collision">
      <geom group="0" type="mesh" conaffinity="1" contype="0" solref="0.02 1" friction="1 0.005 0.0001" condim="4"/>
      <site group="0"/>
    </default>

    <default class="finger">
      <position ctrlrange="0 1.18" kp="1000" kv="124" forcelimited="true" forcerange="-20 20" />
      <joint armature="1.0" limited="true" damping="20" frictionloss="1.0"/>
    </default>
  </default>

  <actuator>
    <position name="gripper_l_finger_joint"  class="finger" joint="rg2_l_finger_joint" />
    <position ctrllimited="true" ctrlrange="0 1.18" joint="rg2_r_finger_joint" kp="1000" name="gripper_r_finger_joint" forcelimited="true" forcerange="-20 20"/>
  </actuator>

  <worldbody>
    <body name="gripper_base" pos="0 0 0" euler="0 0 0">

    <site name="ft_frame" pos="0 0 0" size="0.01 0.01 0.01" rgba="1 0 0 1" type="sphere" group="1" />

      <body name="eef" pos="0 0 0" euler="1.570796325 0 1.570796325">

        <!-- This site was added for visualization. -->
        <site name="grip_site" pos="0 0 0" size="0.01 0.01 0.01" rgba="1 0 0 0.5" type="sphere" group="1"/>
        <!-- This site was added for visualization. -->
        <site name="grip_site_cylinder" pos="0 0 0.1399" size="0.005 10" rgba="0 1 0 0.3" type="cylinder" group="1"/>

        <!-- Actual gripper from here -->
        <!-- <geom name="rg2_hand_collision" class="visual" pos="0 0 0" mesh="hand"/> -->
        <geom name="rg2_hand_visual" class="visual" pos="0 0 0" mesh="hand_vis"/> 

        <inertial pos="0.057581 -0.000169308 -0.000033334" euler="0 0 0" mass="0.29881464108881906" 
          diaginertia="0.00010934311942148434 0.0003327568173229071 0.00037338963627732164"
        />

        <body name="rg2_l_finger" pos="0.105 0.017 0" euler="0 0 0">
          <joint name="rg2_l_finger_joint" class="finger" axis="0 0 1" range="0 1.18" />
          <geom name="rg2_l_finger_col" class="finger_collision" mesh="finger"/>
          <geom name="rg2_l_finger_vis" class="visual" mesh="finger_vis" />
          <inertial pos="-0.0176873 -0.00819142 -4.15573e-05" euler="0 0 0" mass="0.24059267945559043" 
            diaginertia="0.000033970932890017096 0.00029230476124314346 0.00030917405435097734"/>
        </body>

        <body name="rg2_r_finger" pos="0.105 -0.017 0" euler="3.141592653589793 0 0">
          <joint name="rg2_r_finger_joint" class="finger" axis="0 0 1" range="0 1.18"/>
          <geom name="rg2_r_finger_col" class="finger_collision"  mesh="finger"/>
          <geom name="rg2_r_finger_vis" class="visual" mesh="finger_vis"/>
          <inertial pos="0.0176873 -0.00819142 -4.15573e-05" euler="3.141592653589793 0 0" mass="0.24059267945559043" 
            diaginertia="0.000033970932890017096 0.00029230476124314346 0.00030917405435097734"/>
        </body>

      </body>
    </body>
  </worldbody>

  <sensor>
    <force name="force_ee" site="ft_frame"/>
    <torque name="torque_ee" site="ft_frame"/>
  </sensor>
</mujoco>
